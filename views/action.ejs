        <div id="page-inner">
                <div class="row">
                    <div class="col-xs-12">
                        <div class="panel panel-default">
                            <div class="booking-form">
                                <form action="/departments/action?_method=PUT" method="POST">
                                   <input type="hidden" name="_method" value="PUT">
                                    <div class="row">
                                    <% include ./partials/messages %>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <span class="form-label">Report Ref No.</span>
                                                <input class="form-control" name="reportID" type="text"
                                                    placeholder="Enter Report Ref No." required>
                                            </div>

                                        </div>
                                    </div>

                                    <h3>Attended Report / Not Attended Report</h3>

                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="form-group">
                                                <div class="form-checkbox">
                                                    <label for="Attended">
                                                        <input type="radio" value="true" id="Attended" name="attended">
                                                        <span></span>Attended
                                                    </label>
                                                    <label for="Not-Attended">
                                                        <input type="radio" value="false" id="Not-Attended" name="attended">
                                                        <span></span>Not Attended
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>


                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="form-group">
                                                <span class="form-label">Action taken / Results</span>
                                                <textarea class="textarea-input" rows="5" name="action" placeholder="Enter action taken" ></textarea>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="form-btn">
                                                <button type="submit" name="submit" class="submit-btn">Submit Action</button>
                                            </div>
                                        </div>
                                    </div>
                                </form>
